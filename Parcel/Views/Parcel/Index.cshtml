@using FubexParcel.Models;

<h1>Fudex Parcel Tracker</h1>

@if (Model.Count == 0)
{
  <p>There are no items in the list.</p>
}
<ul>
  @foreach (Parcel item in Model)
  {
    <li>@item.Cost</li>
    <li>@item.Length</li>
    <li>@item.Width</li>
    <li>@item.Height</li>
    <li>@item.Weight</li>
    <li>@item.Volume</li>
  }
</ul>
<a href="/parcel/new">Add a new item.</a>